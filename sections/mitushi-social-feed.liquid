{% comment %}
  MITUSHI - Social Feed (Instagram / TikTok embeds)
  Paste official embed codes into blocks via Theme Editor.
  Shows thumbnail with play button, opens modal on click.
{% endcomment %}

<section class="mitushi-social section color-{{ section.settings.color_scheme }} {{ section.settings.remove_padding_top | replace: 'true', 'section-top-zero' | replace: false, '' }} {{ section.settings.remove_padding_bottom | replace: 'true', 'section-bottom-zero' | replace: false, '' }}" data-ui-component="Section Social Feed" style="--ms-gap: {{ section.settings.gap | default: 18 }}px;">
  <div class="ctnr">
    {% if section.settings.heading != blank %}
      <div class="s-header ta-center mb-32">
        <h2 class="mitushi-social__heading h2">{{ section.settings.heading }}</h2>
      </div>
    {% endif %}

    {% if section.blocks.size > 0 %}
      <div class="mitushi-social__rail" aria-label="{{ section.settings.heading | escape }}">
        {% for block in section.blocks %}
          <div class="mitushi-social__card" {{ block.shopify_attributes }}>
            {% if block.settings.embed_code != blank %}
              {%- liquid
                assign embed_code = block.settings.embed_code
                assign post_url = ''
                
                comment
                  Extract Instagram/TikTok URL from embed code (multiple methods)
                endcomment
                
                comment
                  Method 1: Extract from data-instgrm-permalink attribute
                endcomment
                if embed_code contains 'data-instgrm-permalink='
                  assign permalink_match = embed_code | split: 'data-instgrm-permalink="'
                  if permalink_match.size > 1
                    assign post_url = permalink_match[1] | split: '"' | first | split: '?' | first
                  endif
                endif
                
                comment
                  Method 2: Extract from href in anchor tag
                endcomment
                if post_url == blank and embed_code contains 'href='
                  assign href_parts = embed_code | split: 'href="'
                  if href_parts.size > 1
                    assign href_url = href_parts[1] | split: '"' | first
                    if href_url contains 'instagram.com' or href_url contains 'tiktok.com'
                      assign post_url = href_url | split: '?' | first
                    endif
                  endif
                endif
                
                comment
                  Method 3: Extract Instagram post/reel/tv ID from URL patterns
                endcomment
                if post_url == blank
                  if embed_code contains 'instagram.com/p/'
                    assign url_parts = embed_code | split: 'instagram.com/p/'
                    if url_parts.size > 1
                      assign post_id = url_parts[1] | split: '/' | first | split: '?' | first | split: '"' | first
                      assign post_url = 'https://www.instagram.com/p/' | append: post_id | append: '/'
                    endif
                  elsif embed_code contains 'instagram.com/reel/'
                    assign url_parts = embed_code | split: 'instagram.com/reel/'
                    if url_parts.size > 1
                      assign post_id = url_parts[1] | split: '/' | first | split: '?' | first | split: '"' | first
                      assign post_url = 'https://www.instagram.com/reel/' | append: post_id | append: '/'
                    endif
                  elsif embed_code contains 'instagram.com/tv/'
                    assign url_parts = embed_code | split: 'instagram.com/tv/'
                    if url_parts.size > 1
                      assign post_id = url_parts[1] | split: '/' | first | split: '?' | first | split: '"' | first
                      assign post_url = 'https://www.instagram.com/tv/' | append: post_id | append: '/'
                    endif
                  elsif embed_code contains 'tiktok.com'
                    assign url_parts = embed_code | split: 'tiktok.com'
                    if url_parts.size > 1
                      assign tiktok_path = url_parts[1] | split: '"' | first | split: "'" | first | split: '?' | first
                      assign post_url = 'https://www.tiktok.com' | append: tiktok_path
                    endif
                  endif
                endif
              -%}
              
              <button 
                type="button" 
                class="mitushi-social__thumbnail-btn" 
                data-modal-trigger="mitushi-modal-{{ block.id }}"
                data-post-url="{{ post_url }}"
                aria-label="Open Instagram post"
              >
                <div class="mitushi-social__thumbnail" data-thumbnail-container="{{ block.id }}">
                  {% if block.settings.thumbnail_url != blank %}
                    {% assign thumbnail_img = block.settings.thumbnail_url %}
                    <img 
                      src="{{ thumbnail_img | image_url: width: 600 }}" 
                      srcset="{{ thumbnail_img | image_url: width: 300 }} 300w, {{ thumbnail_img | image_url: width: 600 }} 600w, {{ thumbnail_img | image_url: width: 900 }} 900w"
                      sizes="(max-width: 480px) 50vw, (max-width: 768px) 33vw, 300px"
                      alt="Instagram post" 
                      class="mitushi-social__thumbnail-img"
                      loading="lazy"
                    />
                  {% else %}
                    <!-- Thumbnail will be extracted from embed code or show placeholder -->
                    <div class="mitushi-social__thumbnail-loading">
                      <svg class="mitushi-social__play-icon" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <circle cx="12" cy="12" r="10" fill="rgba(0, 0, 0, 0.3)"/>
                        <path d="M10 8L16 12L10 16V8Z" fill="white"/>
                      </svg>
                    </div>
                  {% endif %}
                  <div class="mitushi-social__play-overlay">
                    <svg class="mitushi-social__play-icon" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                      <circle cx="12" cy="12" r="10" fill="rgba(0, 0, 0, 0.5)"/>
                      <path d="M10 8L16 12L10 16V8Z" fill="white"/>
                    </svg>
                  </div>
                </div>
              </button>

              <!-- Template for embed code (hidden, loaded only when modal opens) -->
              <template id="embed-template-{{ block.id }}" style="display: none !important; visibility: hidden !important;">
                {{ block.settings.embed_code }}
              </template>

              <!-- Modal -->
              <div 
                class="mitushi-social__modal" 
                id="mitushi-modal-{{ block.id }}"
                role="dialog"
                aria-modal="true"
                aria-labelledby="modal-title-{{ block.id }}"
                hidden
                data-embed-template="embed-template-{{ block.id }}"
              >
                <div class="mitushi-social__modal-overlay" data-modal-close="mitushi-modal-{{ block.id }}"></div>
                <div class="mitushi-social__modal-content">
                  <button 
                    class="mitushi-social__modal-close" 
                    type="button"
                    data-modal-close="mitushi-modal-{{ block.id }}"
                    aria-label="Close modal"
                  >
                    <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                      <path d="M18 6L6 18M6 6L18 18" stroke="currentColor" stroke-width="2" stroke-linecap="round"/>
                    </svg>
                  </button>
                  <div class="mitushi-social__modal-embed" id="modal-embed-{{ block.id }}">
                    <!-- Embed code will be loaded here when modal opens -->
                  </div>
                </div>
              </div>
            {% else %}
              <div class="mitushi-social__empty">
                <p class="ta-center">Add embed code in the block settings.</p>
              </div>
            {% endif %}

            {% if block.settings.caption != blank %}
              <p class="mitushi-social__caption">{{ block.settings.caption }}</p>
            {% endif %}
            <div class="mitushi-social__handle" data-handle-container="{{ block.id }}"></div>
          </div>
        {% endfor %}
      </div>
    {% else %}
      <div class="mitushi-social__empty-state ta-center">
        <p>No posts yet. Add blocks in the section settings.</p>
      </div>
    {% endif %}
  </div>
</section>

<style>
  .mitushi-social {
    padding: 48px 0;
  }

  .mitushi-social__heading {
    text-align: center;
    margin: 0 0 18px 0;
    letter-spacing: 0.08em;
    text-transform: uppercase;
    font-weight: 800;
  }

  /* Grid layout like Instafeed App */
  .mitushi-social__rail {
    display: grid;
    grid-template-columns: repeat(auto-fill, minmax(280px, 1fr));
    gap: var(--ms-gap, 18px);
    padding: 10px 0;
    max-width: 100%;
  }

  @media (min-width: 1200px) {
    .mitushi-social__rail {
      grid-template-columns: repeat(auto-fill, minmax(300px, 1fr));
    }
  }

  @media (max-width: 768px) {
    .mitushi-social__rail {
      grid-template-columns: repeat(auto-fill, minmax(150px, 1fr));
      gap: calc(var(--ms-gap, 18px) * 0.7);
    }
  }

  @media (max-width: 480px) {
    .mitushi-social__rail {
      grid-template-columns: repeat(2, 1fr);
      gap: 8px;
    }
  }

  .mitushi-social__card {
    width: 100%;
    position: relative;
  }

  /* Hide template elements completely */
  template {
    display: none !important;
    visibility: hidden !important;
  }

  /* Hide any embed code that might render in the card (shouldn't happen, but just in case) */
  .mitushi-social__card > *:not(.mitushi-social__thumbnail-btn):not(.mitushi-social__modal):not(.mitushi-social__caption):not(.mitushi-social__handle):not(.mitushi-social__empty):not(template) {
    display: none !important;
  }

  /* Thumbnail Button */
  .mitushi-social__thumbnail-btn {
    width: 100%;
    padding: 0;
    border: none;
    background: transparent;
    cursor: pointer;
    position: relative;
    border-radius: 8px;
    overflow: hidden;
    transition: transform 0.3s ease, box-shadow 0.3s ease;
  }

  .mitushi-social__thumbnail-btn:hover {
    transform: translateY(-4px);
    box-shadow: 0 8px 16px rgba(0, 0, 0, 0.15);
  }

  .mitushi-social__thumbnail {
    position: relative;
    width: 100%;
    aspect-ratio: 1 / 1;
    overflow: hidden;
    border-radius: 8px;
    background: #f5f5f5;
  }

  .mitushi-social__thumbnail img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    display: block;
    transition: transform 0.3s ease;
  }

  .mitushi-social__thumbnail-btn:hover .mitushi-social__thumbnail img {
    transform: scale(1.05);
  }

  .mitushi-social__play-overlay {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    display: flex;
    align-items: center;
    justify-content: center;
    background: rgba(0, 0, 0, 0.1);
    transition: background 0.3s ease, opacity 0.3s ease;
    opacity: 0;
  }

  .mitushi-social__thumbnail-btn:hover .mitushi-social__play-overlay {
    background: rgba(0, 0, 0, 0.3);
    opacity: 1;
  }

  .mitushi-social__play-icon {
    width: 48px;
    height: 48px;
    filter: drop-shadow(0 2px 8px rgba(0, 0, 0, 0.4));
    transition: transform 0.3s ease, opacity 0.3s ease;
    opacity: 0.9;
  }

  .mitushi-social__thumbnail-btn:hover .mitushi-social__play-icon {
    transform: scale(1.15);
    opacity: 1;
  }

  .mitushi-social__thumbnail-loading {
    width: 100%;
    aspect-ratio: 1 / 1;
    background: linear-gradient(135deg, rgba(var(--color-normal-text-rgb), 0.05) 0%, rgba(var(--color-normal-text-rgb), 0.02) 100%);
    border-radius: 8px;
    display: flex;
    align-items: center;
    justify-content: center;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    animation: pulse 2s ease-in-out infinite;
  }

  @keyframes pulse {
    0%, 100% {
      opacity: 1;
    }
    50% {
      opacity: 0.7;
    }
  }

  .mitushi-social__thumbnail-loading .mitushi-social__play-icon {
    width: 48px;
    height: 48px;
  }

  .mitushi-social__thumbnail-img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    display: block;
  }

  /* Modal */
  .mitushi-social__modal {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 9999;
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 20px;
    opacity: 0;
    visibility: hidden;
    transition: opacity 0.3s ease, visibility 0.3s ease;
  }

  .mitushi-social__modal[hidden] {
    display: none !important;
  }

  .mitushi-social__modal:not([hidden]) {
    opacity: 1;
    visibility: visible;
  }

  .mitushi-social__modal-overlay {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.8);
    backdrop-filter: blur(4px);
    cursor: pointer;
  }

  .mitushi-social__modal-content {
    position: relative;
    background: #fff;
    border-radius: 16px;
    max-width: 540px;
    width: 100%;
    max-height: 90vh;
    overflow-y: auto;
    box-shadow: 0 20px 60px rgba(0, 0, 0, 0.3);
    z-index: 1;
    animation: modalSlideIn 0.3s ease;
  }

  @keyframes modalSlideIn {
    from {
      transform: translateY(20px);
      opacity: 0;
    }
    to {
      transform: translateY(0);
      opacity: 1;
    }
  }

  .mitushi-social__modal-close {
    position: absolute;
    top: 12px;
    right: 12px;
    width: 36px;
    height: 36px;
    border-radius: 50%;
    border: none;
    background: rgba(0, 0, 0, 0.6);
    color: white;
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 10;
    transition: background 0.3s ease, transform 0.3s ease;
  }

  .mitushi-social__modal-close:hover {
    background: rgba(0, 0, 0, 0.8);
    transform: rotate(90deg);
  }

  .mitushi-social__modal-close svg {
    width: 20px;
    height: 20px;
    stroke: currentColor;
  }

  .mitushi-social__modal-embed {
    padding: 20px;
    border-radius: 16px;
    overflow: hidden;
    min-height: 400px;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  /* Make embedded iframes responsive - ONLY in modal */
  .mitushi-social__modal-embed iframe,
  .mitushi-social__modal-embed blockquote,
  .mitushi-social__modal-embed .instagram-media {
    width: 100% !important;
    max-width: 100% !important;
    border: none !important;
    margin: 0 !important;
    display: block !important;
    visibility: visible !important;
    opacity: 1 !important;
    height: auto !important;
    min-width: 326px !important;
  }

  .mitushi-social__modal-embed .instagram-media {
    background: #fff !important;
    border: 0 !important;
    border-radius: 3px !important;
    box-shadow: 0 0 1px 0 rgba(0,0,0,0.5), 0 1px 10px 0 rgba(0,0,0,0.15) !important;
    margin: 1px !important;
    max-width: 540px !important;
    min-width: 326px !important;
    padding: 0 !important;
    width: calc(100% - 2px) !important;
  }

  .mitushi-social__caption {
    margin: 10px 0 0;
    font-size: 14px;
    opacity: 0.85;
    text-align: left;
    line-height: 1.5;
  }

  .mitushi-social__handle {
    margin-top: 6px;
    font-size: 11px;
    color: rgba(var(--color-normal-text-rgb), 0.6);
    text-align: left;
    font-weight: 500;
    letter-spacing: 0.02em;
  }

  .mitushi-social__handle:empty {
    display: none;
  }

  .mitushi-social__empty {
    border-radius: 14px;
    padding: 48px 18px;
    background: rgba(var(--color-normal-text-rgb), 0.04);
    font-size: 14px;
    opacity: 0.75;
    min-height: 200px;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .mitushi-social__empty-state {
    padding: 48px 20px;
    background: rgba(var(--color-normal-text-rgb), 0.02);
    border-radius: 8px;
  }

  @media (min-width: 990px) {
    .mitushi-social__card {
      width: 340px;
    }
  }

  @media (max-width: 768px) {
    .mitushi-social {
      padding: 32px 0;
    }

    .mitushi-social__heading {
      font-size: var(--heading-3);
      margin-bottom: 24px;
    }

    .mitushi-social__modal-content {
      max-width: 95vw;
      max-height: 95vh;
    }

    .mitushi-social__play-icon {
      width: 48px;
      height: 48px;
    }
  }
</style>

<script>
  (function() {
    // Extract thumbnail from embed code (simple method, no API calls)
    function extractThumbnailFromEmbed(container, blockId) {
      if (!container) return;
      
      // Check if thumbnail already exists (from manual image picker)
      const existingImg = container.querySelector('.mitushi-social__thumbnail-img');
      if (existingImg && existingImg.src && !existingImg.src.includes('data:')) {
        // Thumbnail already loaded from manual image picker
        const loadingDiv = container.querySelector('.mitushi-social__thumbnail-loading');
        if (loadingDiv) {
          loadingDiv.remove();
        }
        return;
      }
      
      // Try to extract from embed code
      const embedTemplate = document.getElementById('embed-template-' + blockId);
      if (!embedTemplate) return;
      
      const embedContent = embedTemplate.content.textContent || embedTemplate.content.innerHTML;
      
      // Try to extract handle from embed code
      const handlePatterns = [
        /@([a-zA-Z0-9_.]+)/g,
        /data-instgrm-permalink="[^"]*\/\/([^\/]+)\/([^\/]+)\//,
        /instagram\.com\/([a-zA-Z0-9_.]+)/,
        /tiktok\.com\/@([a-zA-Z0-9_.]+)/,
        /by\s+([a-zA-Z0-9_.]+)/i
      ];
      
      for (let pattern of handlePatterns) {
        const match = embedContent.match(pattern);
        if (match) {
          const handle = match[1] || match[2];
          if (handle && !handle.includes('instagram.com') && !handle.includes('tiktok.com') && !handle.includes('http')) {
            displayInstagramHandle('@' + handle, blockId);
            break;
          }
        }
      }
      
      // Try to find thumbnail URL in embed code (if Instagram/TikTok includes it)
      const thumbnailPatterns = [
        /thumbnail_url["\s:=]+([^"'\s>]+)/i,
        /poster=["']([^"']+)/i,
        /src=["']([^"']*\.(jpg|jpeg|png|webp)[^"']*)/i,
        /background-image:\s*url\(["']?([^"')]+\.(jpg|jpeg|png|webp)[^"')]*)/i
      ];
      
      for (let pattern of thumbnailPatterns) {
        const match = embedContent.match(pattern);
        if (match && match[1]) {
          loadThumbnailImage(match[1], container);
          return;
        }
      }
      
      // If no thumbnail found in embed code, keep the loading placeholder
      // User can add thumbnail URL manually in settings
    }

    function displayInstagramHandle(authorName, blockId) {
      const handleContainer = document.querySelector(`[data-handle-container="${blockId}"]`);
      if (handleContainer && authorName) {
        // Extract handle from author name or URL
        let handle = authorName.trim();
        if (!handle.startsWith('@')) {
          handle = '@' + handle;
        }
        // Clean up handle (remove spaces, keep only valid characters)
        handle = handle.replace(/\s+/g, '').replace(/[^@a-zA-Z0-9_.]/g, '');
        if (handle && handle.length > 1) {
          handleContainer.textContent = handle;
        }
      }
    }

    function loadThumbnailImage(thumbnailUrl, container) {
      if (!thumbnailUrl || !container) return;
      
      // Remove any existing image first (only if it's from embed code extraction)
      const existingImg = container.querySelector('.mitushi-social__thumbnail-img');
      if (existingImg && (!existingImg.src || existingImg.src.includes('data:'))) {
        existingImg.remove();
      } else if (existingImg) {
        // Manual image already exists, don't replace it
        return;
      }
      
      const img = document.createElement('img');
      img.src = thumbnailUrl;
      img.alt = 'Instagram post';
      img.loading = 'lazy';
      img.className = 'mitushi-social__thumbnail-img';
      img.style.width = '100%';
      img.style.height = '100%';
      img.style.objectFit = 'cover';
      img.style.display = 'block';
      
      img.onerror = function() {
        this.remove();
        console.log('Failed to load thumbnail from embed code:', thumbnailUrl);
      };
      
      img.onload = function() {
        const loadingDiv = container.querySelector('.mitushi-social__thumbnail-loading');
        if (loadingDiv) {
          loadingDiv.style.opacity = '0';
          loadingDiv.style.transition = 'opacity 0.3s ease';
          setTimeout(() => {
            loadingDiv.remove();
          }, 300);
        }
      };
      
      container.appendChild(img);
    }

    // Modal functionality
    document.addEventListener('DOMContentLoaded', function() {
      const triggers = document.querySelectorAll('[data-modal-trigger]');
      const closes = document.querySelectorAll('[data-modal-close]');

      // Try to extract thumbnails from embed code (if not manually set)
      triggers.forEach(trigger => {
        const container = trigger.querySelector('[data-thumbnail-container]');
        const blockId = container?.getAttribute('data-thumbnail-container');
        
        if (container && blockId) {
          // Small delay to ensure DOM is ready
          setTimeout(() => {
            extractThumbnailFromEmbed(container, blockId);
          }, 100);
        }
      });

      triggers.forEach(trigger => {
        trigger.addEventListener('click', function(e) {
          e.preventDefault();
          const modalId = this.getAttribute('data-modal-trigger');
          const modal = document.getElementById(modalId);
          if (modal) {
            // Load embed code from template only when modal opens
            const embedContainer = modal.querySelector('.mitushi-social__modal-embed');
            const templateId = modal.getAttribute('data-embed-template');
            
            if (embedContainer && templateId && embedContainer.children.length === 0) {
              const template = document.getElementById(templateId);
              if (template) {
                const embedContent = template.content.cloneNode(true);
                embedContainer.appendChild(embedContent);
                
                // Load Instagram embed script if not already loaded
                if (!window.instgrm) {
                  const script = document.createElement('script');
                  script.src = '//www.instagram.com/embed.js';
                  script.async = true;
                  script.onload = function() {
                    if (window.instgrm && window.instgrm.Embeds) {
                      window.instgrm.Embeds.process();
                    }
                  };
                  document.body.appendChild(script);
                } else if (window.instgrm.Embeds) {
                  // Re-initialize Instagram embeds
                  window.instgrm.Embeds.process();
                }
              }
            }
            
            modal.removeAttribute('hidden');
            document.body.style.overflow = 'hidden';
          }
        });
      });

      closes.forEach(close => {
        close.addEventListener('click', function() {
          const modalId = this.getAttribute('data-modal-close');
          const modal = document.getElementById(modalId);
          if (modal) {
            modal.setAttribute('hidden', '');
            document.body.style.overflow = '';
          }
        });
      });

      // Close on ESC key
      document.addEventListener('keydown', function(e) {
        if (e.key === 'Escape') {
          const openModal = document.querySelector('.mitushi-social__modal:not([hidden])');
          if (openModal) {
            openModal.setAttribute('hidden', '');
            document.body.style.overflow = '';
          }
        }
      });
    });
  })();
</script>

{% schema %}
{
  "name": "MITUSHI - Social Feed",
  "tag": "section",
  "class": "section",
  "settings": [
    {
      "type": "header",
      "content": "t:general.general"
    },
    {
      "type": "color_scheme",
      "id": "color_scheme",
      "label": "t:general.color_scheme",
      "default": "accent-1"
    },
    {
      "type": "text",
      "id": "heading",
      "label": "Heading",
      "default": "INSTAGRAM POSTS"
    },
    {
      "type": "range",
      "id": "gap",
      "label": "Gap between cards (px)",
      "min": 8,
      "max": 40,
      "step": 2,
      "default": 18,
      "info": "This will be applied as CSS variable --ms-gap"
    },
    {
      "type": "header",
      "content": "t:general.layout"
    },
    {
      "type": "checkbox",
      "id": "remove_padding_top",
      "label": "t:general.remove_padding_top",
      "default": false
    },
    {
      "type": "checkbox",
